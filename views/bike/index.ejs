<script>
axios.get(`/api/v1/bikes`)
  .then(function (response) {
		response = response.data;
		for(d of response){
			$("#listBikes").append(`<div class="col-xl-4 col-md-6" ><div class="product"><div class="product_image"><a href ="/bikes/${d._id}"><img src= ${d.image_url} alt=""></a></div>
						<div class="product_content">
							<div class="product_info d-flex flex-row align-items-start justify-content-start">
								<div>
									<div>
										<div class="product_name"><a href="/bikes/${d._id}">${d.name}${d.engine}</a></div>
										<div class="product_category">Make Year: ${d.year}  <!--<a href="category.html">Category</a>--></div>
									</div>
								</div>
								<div class="ml-auto text-right">
									<!--<div class="rating_r rating_r_4 home_item_rating"><i></i><i></i><i></i><i></i><i></i></div>-->
									<div class="product_price text-right">Rs. ${d.price}<!--<span>.99</span>--></div>
								</div>
							</div>
							</div></div></div>`);
						}
  })
  .catch(function (error) {
    // handle error
    console.log(error,"a");
  })
</script>
